    },
    "version": "0.1.0"
  },
  "paths": {
    "/houses": {
      "get": {
        "tags": [
          "house"
        ],
        "operationId": "get_houses",
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "description": "Page number",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 0
            }
          },
          {
            "name": "page_size",
            "in": "query",
            "description": "Number of items per page",
            "required": false,
            "schema": {
              "type": "integer",
              "minimum": 0
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PaginatedResponse_HouseResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/houses/{id}": {
      "get": {
        "tags": [
          "house"
        ],
        "operationId": "get_house_by_id",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "House ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HouseResponse"
                }
              }
            }
          },
          "404": {
            "description": "House not found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/maps/map": {
      "post": {
        "tags": [
          "map"
        ],
        "operationId": "get_map",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MapRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MapResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/maps/requirement": {
      "post": {
        "tags": [
          "map"
        ],
        "operationId": "post_requirement",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RequirementRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RequirementResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ErrorResponse": {
        "type": "object",
        "required": [
          "error"
        ],
        "properties": {
          "error": {
            "type": "string"
          }
        }
      },
      "HouseResponse": {
        "type": "object",
        "required": [
          "id",
          "address",
          "url"
        ],
        "properties": {
          "address": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "lat": {
            "type": [
              "number",
              "null"
            ],
            "format": "double"
          },
          "lon": {
            "type": [
              "number",
              "null"
            ],
            "format": "double"
          },
          "url": {
            "type": "string"
          }
        }
      },
      "Location": {
        "type": "object",
        "required": [
          "id",
          "h3_index"
        ],
        "properties": {
          "h3_index": {
            "type": "string"
          },
          "id": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "MapRequest": {
        "type": "object",
        "required": [
          "city_code",
          "requirement_ids"
        ],
        "properties": {
          "city_code": {
            "type": "string"
          },
          "requirement_ids": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uuid"
            }
          }
        }
      },
      "MapResponse": {
        "type": "object",
        "required": [
          "tiles"
        ],
        "properties": {
          "tiles": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/MapTileResponse"
            }
          }
        }
      },
      "MapTileResponse": {
        "type": "object",
        "required": [
          "h3_index"
        ],
        "properties": {
          "h3_index": {
            "type": "string"
          }
        }
      },
      "PaginatedResponse_HouseResponse": {
        "type": "object",
        "required": [
          "items",
          "total_items",
          "total_pages",
          "current_page",
          "page_size"
        ],
        "properties": {
          "current_page": {
            "type": "integer",
            "minimum": 0
          },
          "items": {
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "id",
                "address",
                "url"
              ],
              "properties": {
                "address": {
                  "type": "string"
                },
                "id": {
                  "type": "integer",
                  "format": "int32"
                },
                "lat": {
                  "type": [
                    "number",
                    "null"
                  ],
                  "format": "double"
                },
                "lon": {
                  "type": [
                    "number",
                    "null"
                  ],
                  "format": "double"
                },
                "url": {
                  "type": "string"
                }
              }
            }
          },
          "page_size": {
            "type": "integer",
            "minimum": 0
          },
          "total_items": {
            "type": "integer",
            "minimum": 0
          },
          "total_pages": {
            "type": "integer",
            "minimum": 0
          }
        }
      },
      "RequirementRequest": {
        "type": "object",
        "required": [
          "requirement_id",
          "country_code",
          "city_code",
          "travel_mode",
          "locations",
          "tolerated_duration"
        ],
        "properties": {
          "city_code": {
            "type": "string"
          },
          "country_code": {
            "type": "string"
          },
          "locations": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Location"
            }
          },
          "requirement_id": {
            "type": "string",
            "format": "uuid"
          },
          "tolerated_duration": {
            "type": "integer",
            "format": "int32"
          },
          "travel_mode": {
            "$ref": "#/components/schemas/TravelMode"
          }
        }
      },
      "RequirementResponse": {
        "type": "object"
      },
      "TravelMode": {
        "type": "string",
        "enum": [
          "Driving",
          "Walking",
          "Bicycling",
          "PublicTransport"
        ]
      }
    }
  },
  "tags": [
    {
      "name": "house",
      "description": "House endpoints"
    },
    {
      "name": "map",
      "description": "Map endpoints"
    }
  ]
}